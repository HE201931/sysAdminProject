acl internals {
    176.96.231.207;
    localhost;
};

view internal {
    match-clients { internals; };
    recursion yes;
    allow-recursion { internals; };
    zone "m1-5.ephec-ti.be" {
        type master;
        file "/etc/bind/db.internal.m1-5.ephec-ti.be";
    };
};

view external {
    match-clients { any; };
    zone "m1-5.ephec-ti.be" {
        type master;
        file "/etc/bind/db.m1-5.ephec-ti.be";
    };
};
